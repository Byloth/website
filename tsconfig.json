{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "lib": [
      "ESNext",
      "DOM",
      "ScriptHost",
      "WebWorker"
    ],
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "noImplicitThis": true,
    "noEmit": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "paths": { "@/*": ["./*"] },
    "types": [
      "@types/node",
      "@nuxt/content",
      "@nuxt/types",
      "@nuxtjs/axios",
      "@nuxtjs/google-analytics"
    ]
  },
  "exclude": [
    ".nuxt",
    "dist",
    "node_modules"
  ]
}
